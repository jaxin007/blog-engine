{
  "name": "blog-engine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "nodemon ./src/index.ts",
    "docker:up": "docker-compose build backend && docker-compose up && npm run migrate:latest ",
    "docker:pg": "docker-compose up postgres && npm run migrate:latest",
    "docker:touch-pg": "docker exec -it blog-engine-postgres psql -U postgres",
    "docker:down": "docker-compose down",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaxin007/blog-engine.git"
  },
  "author": "jaxin007",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaxin007/blog-engine/issues"
  },
  "homepage": "https://github.com/jaxin007/blog-engine#readme",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node -r dotenv/config ./src/index.ts"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fastest-validator": "^1.7.0",
    "knex": "^0.21.5",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.2",
    "@types/cors": "^2.8.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  }
}
